<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://article.nure.ua/entity/article/"
  	xmlns:tns="http://article.nure.ua/entity/article/"
	elementFormDefault="qualified">
	
	<xsd:include schemaLocation="Article.xsd"/>
	<xsd:element name="Articles">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Article" type="tns:Article"
						minOccurs="0" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
 </xsd:schema>